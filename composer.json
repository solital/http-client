{
    "name": "solital/http-client",
    "description": "HTTP Client for Solital Framework",
    "license": "MIT",
    "keywords": [
        "http-client",
        "solital"
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Solital\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Solital\\Test\\": "tests/"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
